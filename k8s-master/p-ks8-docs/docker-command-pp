"Dockerfile" 7L, 384B  zz           FROM  centos:7
RUN   yum install httpd python36 -y && sed -i "s/Listen 80/Listen 8080/g" /etc/httpd/conf/httpd.conf &&  echo "print('Hello, world!')" >  mail.py  &&  chmod uu+x mail.py && chown apache:apache  /var/run/httpd  /var/log/httpd && chmod  -R 777  /var/run/httpd  /var/log/httpdCOPY  src/  /var/www/html/
EXPOSE 8080
USER apache
CMD ["/usr/sbin/httpd", "-D", "FOREGROUND"]

