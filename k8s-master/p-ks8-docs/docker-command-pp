--More--(46%)
--More--(46%)<IfModule unixd_module>
--More--(46%)#
--More--(46%)# If you wish httpd to run as a different user or group, you must run
--More--(47%)# httpd as root initially and it will switch.  
--More--(47%)#
--More--(47%)# User/Group: The name (or #number) of the user/group to run httpd as.
--More--(47%)# It is usually good practice to create a dedicated user and group for
--More--(48%)# running httpd, as with most system services.
--More--(48%)#
--More--(48%)User www-data
--More--(48%)Group www-data
--More--(48%)
--More--(48%)</IfModule>
--More--(48%)
--More--(48%)# 'Main' server configuration
--More--(48%)#
--More--(48%)# The directives in this section set up the values used by the 'main'
--More--(49%)# server, which responds to any requests that aren't handled by a
--More--(49%)# <VirtualHost> definition.  These values also provide defaults for
--More--(49%)# any <VirtualHost> containers you may define later in the file.
--More--(49%)#
--More--(49%)# All of these directives may appear inside <VirtualHost> containers,
--More--(50%)# in which case these default settings will be overridden for the
--More--(50%)# virtual host being defined.
--More--(50%)#
--More--(50%)
--More--(50%)#
--More--(50%)# ServerAdmin: Your address, where problems with the server should be
--More--(51%)# e-mailed.  This address appears on some server-generated pages, such
--More--(51%)# as error documents.  e.g. admin@your-domain.com
--More--(51%)#
--More--(51%)ServerAdmin you@example.com
--More--(51%)
--More--(51%)#
--More--(51%)# ServerName gives the name and port that the server uses to identify itself.
--More--(52%)# This can often be determined automatically, but we recommend you specify
--More--(52%)# it explicitly to prevent problems during startup.
--More--(52%)#
--More--(52%)# If your host doesn't have a registered DNS name, enter its IP address here.
--More--(53%)#
--More--(53%)#ServerName www.example.com:80
--More--(53%)
--More--(53%)#
--More--(53%)# Deny access to the entirety of your server's filesystem. You must
--More--(53%)# explicitly permit access to web content directories in other 
--More--(54%)# <Directory> blocks below.
--More--(54%)#
--More--(54%)<Directory />
--More--(54%)    AllowOverride none
--More--(54%)    Require all denied
--More--(54%)</Directory>
--More--(54%)
--More--(54%)#
--More--(54%)# Note that from this point forward you must specifically allow
--More--(54%)# particular features to be enabled - so if something's not working as
--More--(55%)# you might expect, make sure that you have specifically enabled it
--More--(55%)# below.
--More--(55%)#
--More--(55%)
--More--(55%)#
--More--(55%)# DocumentRoot: The directory out of which you will serve your
--More--(55%)# documents. By default, all requests are taken from this directory, but
--More--(56%)# symbolic links and aliases may be used to point to other locations.
--More--(56%)#
--More--(56%)DocumentRoot "/usr/local/apache2/htdocs"
--More--(56%)<Directory "/usr/local/apache2/htdocs">
--More--(56%)    #
--More--(57%)    # Possible values for the Options directive are "None", "All",
--More--(57%)    # or any combination of:
--More--(57%)    #   Indexes Includes FollowSymLinks SymLinksifOwnerMatch ExecCGI MultiViews
--More--(57%)    #
--More--(57%)    # Note that "MultiViews" must be named *explicitly* --- "Options All"
--More--(58%)    # doesn't give it to you.
--More--(58%)    #
--More--(58%)    # The Options directive is both complicated and important.  Please see
--More--(58%)    # http://httpd.apache.org/docs/2.4/mod/core.html#options
--More--(59%)    # for more information.
--More--(59%)    #
--More--(59%)    Options Indexes FollowSymLinks
--More--(59%)
--More--(59%)    #
--More--(59%)    # AllowOverride controls what directives may be placed in .htaccess files.
--More--(59%)    # It can be "All", "None", or any combination of the keywords:
--More--(60%)    #   AllowOverride FileInfo AuthConfig Limit
--More--(60%)    #
--More--(60%)    AllowOverride None
--More--(60%)
--More--(60%)    #
--More--(60%)    # Controls who can get stuff from this server.
--More--(60%)    #
--More--(60%)    Require all granted
--More--(60%)</Directory>
--More--(60%)
--More--(60%)#
--More--(61%)# DirectoryIndex: sets the file that Apache will serve if a directory
--More--(61%)# is requested.
--More--(61%)#
--More--(61%)<IfModule dir_module>
--More--(61%)    DirectoryIndex index.html
--More--(61%)</IfModule>
--More--(61%)
--More--(61%)#
--More--(61%)# The following lines prevent .htaccess and .htpasswd files from being 
--More--(62%)# viewed by Web clients. 
--More--(62%)#
--More--(62%)<Files ".ht*">
--More--(62%)    Require all denied
--More--(62%)</Files>
--More--(62%)
--More--(62%)#
--More--(62%)# ErrorLog: The location of the error log file.
--More--(62%)# If you do not specify an ErrorLog directive within a <VirtualHost>
--More--(63%)# container, error messages relating to that virtual host will be
--More--(63%)# logged here.  If you *do* define an error logfile for a <VirtualHost>
--More--(63%)# container, that host's errors will be logged there and not here.
--More--(64%)#
--More--(64%)ErrorLog /proc/self/fd/2
--More--(64%)
--More--(64%)#
--More--(64%)# LogLevel: Control the number of messages logged to the error_log.
--More--(64%)# Possible values include: debug, info, notice, warn, error, crit,
--More--(64%)# alert, emerg.
--More--(64%)#
--More--(64%)LogLevel warn
--More--(64%)
--More--(64%)<IfModule log_config_module>
--More--(65%)    #
--More--(65%)    # The following directives define some format nicknames for use with
--More--(65%)    # a CustomLog directive (see below).
--More--(65%)    #
--More--(65%)    LogFormat "%h %l %u %t \"%r\" %>s %b \"%{Referer}i\" \"%{User-Agent}i\"" combined
--More--(66%)    LogFormat "%h %l %u %t \"%r\" %>s %b" common
--More--(66%)
--More--(66%)    <IfModule logio_module>
--More--(66%)      # You need to enable mod_logio.c to use %I and %O
--More--(66%)      LogFormat "%h %l %u %t \"%r\" %>s %b \"%{Referer}i\" \"%{User-Agent}i\" %I %O" combinedio
--More--(67%)    </IfModule>
--More--(67%)
--More--(67%)    #
--More--(67%)    # The location and format of the access logfile (Common Logfile Format).
--More--(67%)    # If you do not define any access logfiles within a <VirtualHost>
--More--(68%)    # container, they will be logged here.  Contrariwise, if you *do*
--More--(68%)    # define per-<VirtualHost> access logfiles, transactions will be
--More--(68%)    # logged therein and *not* in this file.
--More--(68%)    #
--More--(68%)    CustomLog /proc/self/fd/1 common
--More--(69%)
--More--(69%)    #
--More--(69%)    # If you prefer a logfile with access, agent, and referer information
--More--(69%)    # (Combined Logfile Format) you can use the following directive.
--More--(69%)    #
--More--(69%)    #CustomLog "logs/access_log" combined
--More--(70%)</IfModule>
--More--(70%)
--More--(70%)<IfModule alias_module>
--More--(70%)    #
--More--(70%)    # Redirect: Allows you to tell clients about documents that used to 
--More--(70%)    # exist in your server's namespace, but do not anymore. The client 
--More--(70%)    # will make a new request for the document at its new location.
--More--(71%)    # Example:
--More--(71%)    # Redirect permanent /foo http://www.example.com/bar
--More--(71%)
--More--(71%)    #
--More--(71%)    # Alias: Maps web paths into filesystem paths and is used to
--More--(72%)    # access content that does not live under the DocumentRoot.
--More--(72%)    # Example:
--More--(72%)    # Alias /webpath /full/filesystem/path
--More--(72%)    #
--More--(72%)    # If you include a trailing / on /webpath then the server will
--More--(72%)    # require it to be present in the URL.  You will also likely
--More--(73%)    # need to provide a <Directory> section to allow access to
--More--(73%)    # the filesystem path.
--More--(73%)
--More--(73%)    #
--More--(73%)    # ScriptAlias: This controls which directories contain server scripts. 
--More--(74%)    # ScriptAliases are essentially the same as Aliases, except that
--More--(74%)    # documents in the target directory are treated as applications and
--More--(74%)    # run by the server when requested rather than as documents sent to the
--More--(75%)    # client.  The same rules about trailing "/" apply to ScriptAlias
--More--(75%)    # directives as to Alias.
--More--(75%)    #
--More--(75%)    ScriptAlias /cgi-bin/ "/usr/local/apache2/cgi-bin/"
--More--(75%)
--More--(75%)</IfModule>
--More--(75%)
--More--(75%)<IfModule cgid_module>
--More--(76%)    #
--More--(76%)    # ScriptSock: On threaded servers, designate the path to the UNIX
--More--(76%)    # socket used to communicate with the CGI daemon of mod_cgid.
--More--(76%)    #
--More--(76%)    #Scriptsock cgisock
--More--(76%)</IfModule>
--More--(76%)
--More--(76%)#
--More--(76%)# "/usr/local/apache2/cgi-bin" should be changed to whatever your ScriptAliased
--More--(77%)# CGI directory exists, if you have that configured.
--More--(77%)#
--More--(77%)<Directory "/usr/local/apache2/cgi-bin">
--More--(77%)    AllowOverride None
--More--(77%)    Options None
--More--(78%)    Require all granted
--More--(78%)</Directory>
--More--(78%)
--More--(78%)<IfModule headers_module>
--More--(78%)    #
--More--(78%)    # Avoid passing HTTP_PROXY environment to CGI's on this or any proxied
--More--(78%)    # backend servers which have lingering "httpoxy" defects.
--More--(79%)    # 'Proxy' request header is undefined by the IETF, not listed by IANA
--More--(79%)    #
--More--(79%)    RequestHeader unset Proxy early
--More--(79%)</IfModule>
--More--(79%)
--More--(79%)<IfModule mime_module>
--More--(79%)    #
--More--(79%)    # TypesConfig points to the file containing the list of mappings from
--More--(80%)    # filename extension to MIME-type.
--More--(80%)    #
--More--(80%)    TypesConfig conf/mime.types
--More--(80%)
--More--(80%)    #
--More--(80%)    # AddType allows you to add to or override the MIME configuration
--More--(80%)    # file specified in TypesConfig for specific file types.
--More--(81%)    #
--More--(81%)    #AddType application/x-gzip .tgz
--More--(81%)    #
--More--(81%)    # AddEncoding allows you to have certain browsers uncompress
--More--(81%)    # information on the fly. Note: Not all browsers support this.
--More--(82%)    #
--More--(82%)    #AddEncoding x-compress .Z
--More--(82%)    #AddEncoding x-gzip .gz .tgz
--More--(82%)    #
--More--(82%)    # If the AddEncoding directives above are commented-out, then you
--More--(82%)    # probably should define those extensions to indicate media types:
--More--(83%)    #
--More--(83%)    AddType application/x-compress .Z
--More--(83%)    AddType application/x-gzip .gz .tgz
--More--(83%)
--More--(83%)    #
--More--(83%)    # AddHandler allows you to map certain file extensions to "handlers":
--More--(83%)    # actions unrelated to filetype. These can be either built into the server
--More--(84%)    # or added with the Action directive (see below)
--More--(84%)    #
--More--(84%)    # To use CGI scripts outside of ScriptAliased directories:
--More--(84%)    # (You will also need to add "ExecCGI" to the "Options" directive.)
--More--(85%)    #
--More--(85%)    #AddHandler cgi-script .cgi
--More--(85%)
--More--(85%)    # For type maps (negotiated resources):
--More--(85%)    #AddHandler type-map var
--More--(85%)
--More--(85%)    #
--More--(85%)    # Filters allow you to process content before it is sent to the client.
--More--(86%)    #
--More--(86%)    # To parse .shtml files for server-side includes (SSI):
--More--(86%)    # (You will also need to add "Includes" to the "Options" directive.)
--More--(86%)    #
--More--(86%)    #AddType text/html .shtml
--More--(86%)    #AddOutputFilter INCLUDES .shtml
--More--(87%)</IfModule>
--More--(87%)
--More--(87%)#
--More--(87%)# The mod_mime_magic module allows the server to use various hints from the
--More--(87%)# contents of the file itself to determine its type.  The MIMEMagicFile
--More--(87%)# directive tells the module where the hint definitions are located.
--More--(88%)#
--More--(88%)#MIMEMagicFile conf/magic
--More--(88%)
--More--(88%)#
--More--(88%)# Customizable error responses come in three flavors:
--More--(88%)# 1) plain text 2) local redirects 3) external redirects
--More--(88%)#
--More--(88%)# Some examples:
--More--(89%)#ErrorDocument 500 "The server made a boo boo."
--More--(89%)#ErrorDocument 404 /missing.html
--More--(89%)#ErrorDocument 404 "/cgi-bin/missing_handler.pl"
